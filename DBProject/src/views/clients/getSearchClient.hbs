<div class="'col-md-4 mx-auto">
    <div class="card" style="width: 50rem;">
        <div class="'card-header">
            <h3>CONSULT EMPLOYEE</h3>
        </div>

        <!--CARD (FORM) FOR CONSULT PARAMETERS-->
        <div class="card-body">
            <form action="getClientsInfo" method="POST">
                <!--CONUTRY-->
                <div class="form-group">
                    <br>
                    <label for="exampleFormControlSelect1">Contry:</label><br>
                    <select name="id_Country" class="form-control" id="id_Country">
                        <option value=0 selected>NO COUNTRY</option>
                        <option value=1 >GERMANY</option>
                        <option value=2 >EEUU</option>
                        <option value=3 >JAPAN</option>
                    </select>
                </div>

                <!-- SELECT JOB -->
                <div class="form-group">
                    <br>
                    <label for="exampleFormControlSelect3">Sub:</label><br>
                    <select name="id_Sub" class="form-control" id="id_Sub">
                        <option value=0 selected>NO SUB</option>
                        {{#each result_subs}}
                        <option value={{this.id_subscription}}>{{this.name_subscription}}</option>
                        {{/each}}
                    </select>
                </div>

                <div class="form-group">
                    <input type="text" name="clientName" class="form-control" placeholder="Name">

                    <div class="form-group">
                        <label for="start">Start date:</label>

                        <input type="date" id="start" name="start" value="2018-07-22" min="2018-01-01"
                            max="2022-12-31">

                        <label for="finish">Finish date:</label>

                        <input type="date" id="finish" name="finish" value="2018-07-22" min="2018-01-01"
                            max="2022-12-31">

                        <div class="form-group">
                            <button class="btn btn-primary btn-block" type="submit">
                                CONSULT
                            </button>
                        </div>

                    </div>

            </form>
        </div>
    </div>


</div>
<div class="'col-md-4 mx-auto">
    <div>
        <br></br>
        <h3>LIST OF CLIENTS:</h3>
    </div>

    <table class="table">
        <thead>
            <tr>
                <th>Code</th>
                <th>Name</th>
                <th>Country</th>
                <th>Subs</th>
                <th>email</th>
                <th>location</th>
                <th>Amount Purchases</th>
                <th>Total in Purchases</th>
                <th>Total in Shipping</th>
                <th></th>
            </tr>
        </thead>

        <tbody id="productTable" class="collection with-header">

            {{#each result_clients}}
            <tr>
                <td>{{id_client}}</td>
                <td>{{name_client}}</td>
                <td>{{name_Country}}</td>
                <td>{{name_subscription}}</td>
                <td>{{email_client}}</td>
                <td>{{location_client}}</td>
                <td>{{amount_purchases}}</td>
                <td>{{mount_purchases_}}</td>
                <td>{{mount_shipping_}}</td>

                <td>
                    <form action="/editEmployee" method="POST">
                        <input type="hidden" name="id_employee" value={{id_employee}}>
                        <button class="btn btn-primary btn-block" type="submit">
                            EDIT
                        </button>
                    </form>
                </td>



            </tr>

            {{/each}}

        </tbody>
</div>